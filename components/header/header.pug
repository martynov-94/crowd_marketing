mixin header()
  header.header
    img.header__logo(src="img/logo.png" width="148" height="45" alt="Логотип Crowd Marketing")
    nav.header__menu.main-menu
      +main-menu()
    .header__auth-btns
      button.header__login-btn= "Вход"
      span.header__auth-delimiter= "/"
      button.header__sign-up-btn= "Регистрация"
    input.header__menu-btn(type="image" src="img/burger_btn.svg")